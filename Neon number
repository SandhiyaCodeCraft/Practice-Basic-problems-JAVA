import java.util.*;
class neonnumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sq=n*n;
        int s=0;
        int r;
        while(sq>0){
            r=sq%10;
            s+=r;
            sq=sq/10;
        }
        if(s==n){
            System.out.println(n+"is a neon number");
        }
        else{
            System.out.println(n+"is not a neon number");
        }
    }
}
