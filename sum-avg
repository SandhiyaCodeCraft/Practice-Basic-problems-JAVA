import java.util.*;
class sumavg{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         int sum=0;
         int avg;
         for(int i=1;i<=n;i++){
             sum+=i;
         }
         System.out.println(sum);
         avg=sum/n;
         System.out.println(avg);
    }
}
