import java.util.*;
class sumfibonacci {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a=0;
        int b=1;
        int c;
        int s=1;
        for(int i=1;i<n-1;i++){
            c=a+b;
            s+=c;
            a=b;
            b=c;
        }
        System.out.println(s);
    }
}
